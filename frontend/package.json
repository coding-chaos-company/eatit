{
  "name": "intarnet-avengers",
  "displayName": "インターネットアベンジャーズ",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "version": "0.0.1",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "ci": "biome ci .",
    "format": "biome check --apply ."
  },
  "dependencies": {
    "plasmo": "0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.245",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.0",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": ["https://*/*"],
    "permissions": ["tabs"]
  },
  "engines": {
    "node": "20.11.1",
    "npm": "use yarn instead",
    "yarn": "4.1.1"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": "last 2 versions, not dead, > 0.2%"
      }
    }
  }
}
